		methods: {
			addSubmit() {
				var _self = this;
				_self.$refs.addForm.validate((valid) => {
					if(valid) {
						_self.$confirm('确认提交吗？', '提示', {}).then(() => {
							_self.addFormVisible = false;
							//							_self.tableData2Loading = true;
//							_self.tableData2.unshift(_self.addForm);
							/*_self.addForm = {
								hostname: '',
								IP: '',
								type: '',
								system: '',
								grade: ''
							};*/
							common_tools.AJAX('POST', common_tools.interfaceurl + 'safe/addAssetData', _self.addForm, function(data) {
								if(data.flag == '0') {
									_self.getData(1);
								} else {
									alert('参数错误，请重新检查');
								}
							}, function(data) {
								console.log(data);
							});
						});
					}
				});
			},
			editRow(id) {
				var _self = this;
				console.log(id);
				_self.expands = [];
			},
			popAddShow() {
				$('.el-popover').show();
			},
			addSubmit2() {
				var _self = this;
				_self.selectTable.service.unshift(_self.addForm2)
				console.log('这里添加行到后台，回调是下面代码');
				_self.addForm2 = {
						port: '',
						protocol: '',
						name: '',
						status: '',
						isIM: ''
					},
					$('.el-popover').hide();
			},
			cancleAdd() {
				$('.el-popover').hide();
			},
			deleteSelection(item, tableData) {
				console.log(item);
				var _self = this;
				common_tools.deleteSelection(item, tableData, _self);
			},
			handleEdit(index, row) {
				this.dialogFormVisible1 = true;
				this.selectTable = row;
			},
			detailDialog(index, row) {
				this.selectTable = row;
				this.detailDialogShow = true;
				this.drawChart01();
				this.drawChart02();
				this.drawChart03();
				this.drawChart04();
			},
			handleSelectionChange1(val) {
				this.multipleSelection1 = val;
			},
			handleSelectionChange2(val) {
				this.multipleSelection2 = val;
			},
			handleCurrentChange(val) {
				this.cur_page = val;
				this.getData(val);
			},
			// 获取row的key值
			getRowKeys(row) {
				return row.id;
			},
			getData(current_page) {
				var _self = this;
				common_tools.AJAX('POST', common_tools.interfaceurl + 'safe/assets', {
					"page":current_page,
					"size":'5',
					"hostName":"",
					"ipAddress":"",
					"deviceType":""
				}, function(data) {
					if(data.flag == '0') {
						_self.tableData2Loading = false;
						_self.total=data.dataCount;
						_self.tableData2 = data.data;
					} else {
						alert('参数错误，请重新检查');
					}
				}, function(data) {
					console.log(data);
				});
			},
			//显示新增界面
			handleAdd() {
				this.addFormVisible = true;
				common_tools.AJAX('POST', common_tools.interfaceurl + 'safe/addAssetData', {}, function(data) {
					if(data.flag==0){
						_self.chart10.hideLoading();
						
					}else{
						alert('传参错误');
					}
				}, function(data) {
					console.log(data);
				});
				/*var eventId='asd';
				alert(eventId);
				this.$router.push({name:'monitorCenter',params:{eventId:eventId}});*/
			},
			//编辑
			editSubmit() {
				this.$refs.editForm.validate((valid) => {
					if(valid) {
						this.$confirm('确认提交吗？', '提示', {}).then(() => {
							this.editLoading = true;
							//NProgress.start();
							let para = Object.assign({}, this.editForm);
							para.birth = (!para.birth || para.birth == '') ? '' : util.formatDate.format(new Date(para.birth), 'yyyy-MM-dd');
							editUser(para).then((res) => {
								this.editLoading = false;
								//NProgress.done();
								this.$message({
									message: '提交成功',
									type: 'success'
								});
								this.$refs['editForm'].resetFields();
								this.dialogFormVisible1 = false;
								this.getData();
							});
						});
					}
				});
			},
